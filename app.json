{
  "expo": {
    "name": "SOS",
    "slug": "SOS",
    "description": "With this app, you can send SOS notifications to your friends. You can also get notifications when someone sends you a SOS.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sosapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover"
    },
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#2C282A"
      }
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#2C282A"
    },
    "primaryColor": "#2C282A",
    "plugins": ["expo-router", "expo-font"]
  }
}
